# Development performance optimizations
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
FAST_REFRESH=true

# Builder.io development settings
BUILDER_DEV_MODE=true
BUILDER_CACHE_SECONDS=300

# Site configuration
NEXT_PUBLIC_SITE_CONTEXT_NAME=builder-app-template

# Sitemap configuration - comma-separated list of paths to exclude from sitemap.xml
# Example: SITEMAP_EXCLUDED_PATHS=/do-not-publish/,/private/,/admin/
SITEMAP_EXCLUDED_PATHS=/do-not-publish/

# Next.js development optimizations
NEXT_PRIVATE_EXPERIMENTAL_WEBPACK_TREE_SHAKING=true
NEXT_PRIVATE_EXPERIMENTAL_WEBPACK_CACHE=true


# https://www.builder.io/c/docs/using-your-api-key
NEXT_PUBLIC_BUILDER_API_KEY=aa26d0ed43ef421da301a1603f38faeb
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_DEFAULT_COMPONENT_IMAGE=https://placehold.co/50x50?text=Component

